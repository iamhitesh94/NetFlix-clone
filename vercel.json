{
    "builds": [
      { "src": "package.json", "use": "@vercel/node" }
    ],
    "version": 2,
    "functions": {
      "api/**/*.js": { "runtime": "nodejs14.x" }
    },
    "build": {
      "env": {
        "NEXT_TELEMETRY_DISABLED": "1"
      },
      "commands": {
        "build": "prisma generate && next build"
      }
    }
  }
  